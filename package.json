{
  "name": "launcher",
  "version": "1.0.0",
  "description": "Launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . launcher --overwrite --asar=true --platform=win32 --arch=ia32 --icon=logo16px.ico --prune=true --out=release-builds --version-string.CompanyName=SpaceCRAFT_VR --version-string.FileDescription=Engineering_Humanitys_Future_In_Space --version-string.ProductName=\"SpaceCRAFT VR\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Henrynaut/launcher.git"
  },
  "author": "Neil McHenry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Henrynaut/launcher/issues"
  },
  "homepage": "https://github.com/Henrynaut/launcher#readme",
  "dependencies": {
    "custom-electron-titlebar": "^3.2.2-hotfix62",
    "electron": "^8.0.3",
    "npm": "^6.14.1"
  },
  "devDependencies": {
    "electron-packager": "^14.2.1"
  }
}
